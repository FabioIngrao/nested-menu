{
  "compilerOptions": {
    "strict": false,
    "target": "es2015",
    "module": "esnext",
    "moduleResolution": "classic",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "alwaysStrict": false,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react"
  }
}
